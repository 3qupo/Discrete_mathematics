<?xml version="1.0" encoding="windows-1251"?>
<task title="lala">
<description>
<![CDATA[Сколькими способами на доске 6 на 5 можно расставить 6 фигур вида Король на нашем поле, так чтобы они не били друг друга
]]></description>
<description-params>
	<param name="n">
 		<value>6</value>
 	</param>
	<param name="m">
 		<value>5</value>
 	</param>
	<param name="rook">
 		<value>-1</value>
 	</param>
</description-params>
 <mathDescription>

<sourceSet>
<set length="${rook}" type="CombinationSet">
<set type="DecartSet">
<set first="1" last="${n}" type="NumericSet"/>
<set first="1" last="${m}" type="NumericSet"/>
</set>
</set>
</sourceSet>
<verifier type="SimpleVerifier">
<function type="And">
<function type="Equals">
<function type="Add">
<for first="1" inc="1" last="${rook}" name="i">
<function type="And">
<function type="Equals">
<function type="Add">
<for first="1" inc="1" last="${rook}" name="j"><function type="And"></function>
</for>
</function>
<constElement>0</constElement>
</function>
</function>
</for>
</function>
<constElement>-1</constElement>
</function>
</function>
</verifier> </mathDescription>
 </task>